<script>
async function copy(e) {
  try {
    const tiles = e.getAttribute('data-tiles')
    await navigator.clipboard.writeText(tiles)
    e.classList.remove('animate')
    void e.offsetWidth
    e.classList.add('animate')
  } catch (err) {
    console.error('copy failed', err)
    alert('Copy failed: ' + err)
  }
}
</script>
