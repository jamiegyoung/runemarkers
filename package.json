{
  "name": "runemarkers",
  "description": "Find and import RuneLite tile markers for Old School RuneScape. Easily search for tile markers by boss, activity or location.",
  "version": "1.0.0",
  "private": true,
  "author": "Jamie Young",
  "license": "MIT",
  "repository": "https://github.com/jamiegyoung/runemarkers",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 4911",
    "test": "tsc && jest",
    "test:dev": "jest --watch --coverage",
    "test:update": "jest --updateSnapshot",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "coverage:coveralls": "jest --coverage && COVERALLS_SERVICE_NAME=jest COVERALLS_GIT_BRANCH=main yarn coveralls < coverage/lcov.info"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "copy-to-clipboard": "3.3.3",
    "fuse.js": "6.6.2",
    "minimist": "^1.2.8",
    "next": "13.4.6",
    "next-seo": "6.0.0",
    "next-sitemap": "^4.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lite-youtube-embed": "^2.3.52",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/react-syntax-highlighter": "15.5.7",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "commitizen": "4.3.0",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
