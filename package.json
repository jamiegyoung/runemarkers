{
  "name": "runemarkers",
  "description": "Find and import RuneLite tile markers for Old School RuneScape. Easily search for tile markers by boss, activity or location.",
  "version": "1.0.0",
  "private": true,
  "author": "Jamie Young",
  "license": "MIT",
  "repository": "https://github.com/jamiegyoung/runemarkers",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 4911",
    "test": "tsc && jest && node ./bin/CheckThumbnails.mjs",
    "test:dev": "jest --watch --coverage",
    "test:update": "jest --updateSnapshot",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "coverage:coveralls": "jest --coverage && COVERALLS_SERVICE_NAME=jest COVERALLS_GIT_BRANCH=main pnpm coveralls < coverage/lcov.info"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "copy-to-clipboard": "3.3.3",
    "eslint-plugin-import": "^2.29.1",
    "fuse.js": "7.0.0",
    "minimist": "^1.2.8",
    "next": "14.0.4",
    "next-seo": "6.5.0",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lite-youtube-embed": "^2.4.0",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "commitizen": "4.3.0",
    "coveralls": "^3.1.1",
    "cz": "^1.8.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-prettier": "5.1.2",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
